{
  "name": "@comicrelief/test-utils",
  "version": "1.5.15",
  "description": "Test Utilities for integration tests.",
  "main": "dist/index.js",
  "repository": "git@github.com:comicrelief/test-utils.git",
  "author": "Comic Relief",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "restricted"
  },
  "sideEffects": false,
  "peerDependencies": {
    "@comicrelief/data-models": "^1.29.6",
    "aws-sdk": "^2.740.0",
    "axios": "^0.20.0"
  },
  "devDependencies": {
    "@comicrelief/data-models": "^1.29.6",
    "@comicrelief/eslint-config": "^1.2.0",
    "@types/jest": "^26.0.12",
    "@types/node": "^14.6.1",
    "@types/sinon": "^9.0.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^4.7.0",
    "aws-sdk": "^2.740.0",
    "axios": "^0.20.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-unicorn": "^23.0.0",
    "jest": "^26.4.2",
    "semantic-release": "^17.1.2",
    "sinon": "^9.0.3",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build:login": "echo \"//registry.npmjs.org/:_authToken=$NPM_TOKEN\" > .npmrc",
    "build:packaging": "cp package.json README.md LICENSE .npmrc ./dist",
    "build:ts": "tsc",
    "build": "yarn build:login && yarn build:ts && yarn build:packaging",
    "lint": "yarn eslint .",
    "test": "jest --colors",
    "test:feat": "yarn test --selectProjects FEAT",
    "test:unit": "yarn test --selectProjects UNIT"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
